	<servlet-mapping>
		<servlet-name>ServiceRemove Name</servlet-name>
		<url-pattern>/ServiceRemove</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ServiceEdit Name</servlet-name>
