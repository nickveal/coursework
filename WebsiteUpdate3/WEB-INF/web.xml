	<servlet-mapping>
		<servlet-name>ServiceAdd Name</servlet-name>
		<url-pattern>/ServiceAdd</url-pattern>
	</servlet-mapping>
	
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

</web-app>
