	</servlet-mapping>
	
	<servlet>
		<servlet-name>CarAdd Name</servlet-name>
		<servlet-class>CarAdd</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>CarAdd Name</servlet-name>
		<url-pattern>/CarAdd</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CarRemove Name</servlet-name>
